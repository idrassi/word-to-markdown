{
  "name": "word-to-markdown",
  "version": "1.0.0",
  "description": "Privacy-first offline Word to Markdown converter for AI/LLMs. 100% client-side, no data upload. Perfect for Ollama, LM Studio, GPT4All. Convert sensitive documents without cloud risks.",
  "type": "module",
  "license": "MIT",
  "author": "Word to Markdown Contributors",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/word-to-markdown"
  },
  "keywords": [
    "word-to-markdown",
    "docx-converter",
    "privacy",
    "offline",
    "local-llm",
    "ollama",
    "lm-studio",
    "gpt4all",
    "ai-privacy",
    "document-converter",
    "client-side",
    "no-upload",
    "markdown",
    "word",
    "docx",
    "private-ai",
    "local-ai",
    "data-privacy",
    "offline-converter",
    "browser-based"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:standalone": "tsc -b && BUILD_STANDALONE=true vite build && mv standalone/index.html standalone/word-to-markdown.html && cd standalone && zip word-to-markdown.zip word-to-markdown.html",
    "preview": "vite preview",
    "lint": "eslint ."
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "mammoth": "^1.8.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-router": "^7.1.0",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^25.0.1",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.2",
    "@types/turndown": "^5.0.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-singlefile": "^2.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
